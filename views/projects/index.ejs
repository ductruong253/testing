<% include ../partials/header %>

<div class="container">
  <header class="jumbotron">
    <div class="container">

      <h1>This is Project Page</h1>
      <p>View our world class Wire Harness product</p>
      <p>
        <a class="btn btn-primary btn-lg" href="/projects/new">Add new Projects</a>
      </p>
    </div>
  </header>
  <div class="row text-center" style="display:flex; flex-wrap:wrap;">
    <div class="col-lg-12">
      <h3>All project information</h3>

      <% apqps.forEach(function(apqp) {%>


          <div class="caption">
            <h4><%= apqp.name %></h4>
          </div>
          <p>
            <a href="/apqp/<%=apqp._id%>" class="btn btn-lg btn-primary">More info</a>
          </p>

      <% }) %>
    </div>
  </div>
</div>

<% include ../partials/footer %>
